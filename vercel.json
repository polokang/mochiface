{
  "functions": {
    "app/api/image/generate/route.ts": {
      "maxDuration": 60
    },
    "app/api/upload-image/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
