# Vercel 部署环境变量配置
# 复制此文件到 Vercel 项目设置中的环境变量

# Supabase 配置
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google API 配置
GOOGLE_API_KEY=your_google_api_key

# Vercel 性能优化配置
IMAGE_GEN_API_TIMEOUT=20000          # Vercel环境20秒超时
IMAGE_GEN_MAX_RETRIES=1              # Vercel环境减少重试次数
IMAGE_GEN_ENABLE_CACHE=true          # 启用缓存
IMAGE_GEN_MAX_IMAGE_SIZE=3145728     # Vercel环境3MB限制
ENABLE_DETAILED_LOGGING=true         # 启用详细日志
LOG_SLOW_REQUESTS=true               # 记录慢请求
SLOW_REQUEST_THRESHOLD=3000          # Vercel环境3秒阈值

# Vercel 特定配置
VERCEL=true                          # 标识Vercel环境
NODE_ENV=production                  # 生产环境
